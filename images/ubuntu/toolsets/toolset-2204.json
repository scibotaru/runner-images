{
    "toolcache": [
        {
            "name": "Python",
            "url" : "https://raw.githubusercontent.com/actions/python-versions/main/versions-manifest.json",
            "platform" : "linux",
            "platform_version": "22.04",
            "arch": "x64",
            "versions": [
                "3.12.*"
            ]
        },
        {
            "name": "PyPy",
            "arch": "x64",
            "platform" : "linux",
            "versions": [
                "3.10"
            ]
        },
        {
            "name": "node",
            "url" : "https://raw.githubusercontent.com/actions/node-versions/main/versions-manifest.json",
            "platform" : "linux",
            "arch": "x64",
            "versions": [
                "14.*",
                "16.*",
                "20.*"
            ]
        }
    ],
    "java": {
        "default": "17",
        "versions": [ "17", "21"],
        "maven": "3.8.8"
    },
    "powershellModules": [
        {"name": "MarkdownPS"},
        {"name": "Microsoft.Graph"},
        {"name": "Pester"},
        {"name": "SqlServer" },
        { "name": "PSScriptAnalyzer"} ,
        {"name": "powershell-yaml"        },
        {            "name": "MicrosoftPowerBIMgmt"        }
    ],
    "azureModules": [
        {
            "name": "az",
            "url" : "https://raw.githubusercontent.com/Azure/az-ps-module-versions/main/versions-manifest.json",
            "versions": [
                "11.3.1"
            ],
            "zip_versions": [
            ]
        }
    ],
    "apt": {
        "vital_packages": [
            "bzip2",
            "curl",
            "g++",
            "gcc",
            "make",
            "jq",
            "tar",
            "unzip",
            "wget"
        ],
        "common_packages": [
            "dnsutils",
            "dpkg",
            "fakeroot",
            "gnupg2",
            "iproute2",
            "iputils-ping",
            "libsecret-1-dev",
            "libsqlite3-dev",
            "libyaml-dev",
            "libtool",
            "libunwind8",
            "libxkbfile-dev",
            "libxss1",
            "libssl-dev",
            "openssh-client",
            "pkg-config",
            "python-is-python3",
            "texinfo",
            "tk",
            "tzdata",
            "upx",
            "xorriso",
            "xvfb",
            "xz-utils",
            "zsync"
        ],
        "cmd_packages": [
            "coreutils",
            "file",
            "findutils",
            "ftp",
            "netcat",
            "net-tools",
            "p7zip-full",
            "pass",
            "rsync",
            "ssh",
            "sshpass",
            "sudo",
            "time",
            "zip"
        ]
    },
    "docker": {
        "images": [
            "ubuntu:22.04"
        ],
        "components": [
            {
                "package": "containerd.io",
                "version": "latest"
            },
            {
                "package": "docker-ce-cli",
                "version": "24.0.9"
            },
            {
                "package": "docker-ce",
                "version": "24.0.9"
            }
        ],
        "plugins": [
            {
                "plugin": "buildx",
                "version": "latest",
                "asset": "linux-amd64"
            },
            {
                "plugin": "compose",
                "version": "2.23.3",
                "asset": "linux-x86_64"
            }
        ]
    },
    "dotnet": {
        "aptPackages": [
            "dotnet-sdk-6.0",
            "dotnet-sdk-8.0"
        ],
        "versions": [
            "6.0",
            "8.0"
        ],
        "tools": [
            { "name": "nbgv", "test": "nbgv --version", "getversion" : "nbgv --version" }
        ]
    },
    "node": {
        "default": "14"
    },
    "node_modules": [
        {
            "name": "n",
            "command": "n"
        },
        {
            "name": "typescript",
            "command": "tsc"
        },
        {
            "name": "webpack",
            "command": "webpack"
        },
        {
            "name": "webpack-cli",
            "command": "webpack-cli"
        },
        {
            "name": "yarn",
            "command": "yarn"
        }
    ],
    "pwsh": {
        "version": "7.4"
    }
}
